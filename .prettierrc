{
	"printWidth": 120,
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"arrowParens": "avoid",
	"proseWrap": "never",
	"htmlWhitespaceSensitivity": "ignore",
	"insertPragma": false,
	"quoteProps": "as-needed",
	"requirePragma": false,
	"vueIndentScriptAndStyle": false,
	"singleAttributePerLine": false
}
